{
  "name": "misc-helpers",
  "version": "2.0.10",
  "author": "Arthur José Germano",
  "license": "MIT",
  "description": "Uma coleção robusta de helpers, validadores e classes de alta performance para JavaScript/Node.js, incluindo processamento em lote (bulk), validadores de documentos brasileiros e utilitários de criptografia.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "readme.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test:watch": "vitest",
    "test:bulk": "NODE_OPTIONS='--expose-gc' vitest run ./tests/bulkProcessor.test.js",
    "test": "NODE_OPTIONS='--expose-gc' vitest --run --reporter verbose --globals",
    "testUtils": "NODE_OPTIONS='--expose-gc' vitest --run --reporter verbose --globals ./tests/utils.test.js",
    "testHelpers": "NODE_OPTIONS='--expose-gc' vitest --run --reporter verbose --globals ./tests/helpers.test.js",
    "testWebAuthn2": "NODE_OPTIONS='--expose-gc' vitest --run --reporter verbose --globals ./tests/webauthn2.test.js",
    "testWebAuthn3": "NODE_OPTIONS='--expose-gc' vitest --run --reporter verbose --globals ./tests/webauthn3.test.js",
    "testBulkProcessor": "NODE_OPTIONS='--expose-gc' vitest --run --reporter verbose --globals ./tests/bulkProcessor.test.js",
    "build": "tsup",
    "build:debug": "tsup --minify false --sourcemap",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run test && npm run build",
    "check-exports": "node -e \"console.log(Object.keys(require('./dist/index.js')))\""
  },
  "keywords": [
    "helpers",
    "utils",
    "validators",
    "bulk",
    "batch",
    "async",
    "performance",
    "cpf",
    "cnpj",
    "pix",
    "cnh",
    "renavam",
    "rg",
    "webauthn",
    "crypto",
    "throttle",
    "debounce",
    "date"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthurgermano/misc-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/arthurgermano/misc-helpers/issues"
  },
  "homepage": "https://github.com/arthurgermano/misc-helpers#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false,
  "dependencies": {
    "cbor-x": "^1.6.0",
    "date-fns": "^4.1.0",
    "fflate": "^0.8.2"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}